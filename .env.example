# Environment
ENV=dev
LOG_LEVEL=info

# Kaspa Network
NETWORK=mainnet
RPC_URL=127.0.0.1:16110

# Database
DB_URI=postgresql://user:pass@localhost/kaspalytics
DB_MAX_POOL_SIZE=10

# Email notifications
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_FROM=noreply@example.com
SMTP_TO=admin@example.com

# Web server
WEB_LISTEN_IP=127.0.0.1
WEB_PORT=3000
ALLOWED_ORIGINS=http://localhost:3000